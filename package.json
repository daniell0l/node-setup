    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.17.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.12.2",
    "ts-node-dev": "^1.1.8"
  }
